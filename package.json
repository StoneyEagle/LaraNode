{
  "name": "nomercy-media-server",
  "description": "The Effortless Encoder",
  "main": "./out/main/src/server/src/index.js",
  "version": "0.0.2",
  "scripts": {
    "format": "prettier --write electron-server",
    "lint": "eslint electron-server --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck": "npm run typecheck:node",
    "start": "electron-vite preview",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 electron-vite dev --watch",
    "build": "electron-vite build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3001/"
    }
  },
  "keywords": [],
  "license": "ISC",
  "_moduleAliases": {
    "@/*": "src/*",
    "@framework/*": "framework/*"
  },
  "author": {
    "name": "NoMercy Entertainment",
    "email": "stoney@nomercy.tv"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^2.0.0",
    "@swc/core": "^1.3.95",
    "@swc/helpers": "^0.5.3",
    "@types/express": "^4.17.20",
    "@types/i18next-node-fs-backend": "^2.1.5",
    "@types/node": "^20.8.10",
    "@types/promise-retry": "^1.1.6",
    "cross-env": "^7.0.3",
    "electron": "^27.0.4",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.28",
    "ignore-loader": "^0.1.2",
    "nodemon": "^3.0.1",
    "openapi-typescript": "^6.7.1",
    "transpile-webpack-plugin": "^1.1.3",
    "tree-kill": "^1.2.2",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.15.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "axios": "^1.6.0",
    "axios-rate-limit": "^1.3.0",
    "bufferutil": "^4.0.8",
    "chalk": "4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http2-express-bridge": "^1.0.7",
    "i18next": "^23.6.0",
    "i18next-http-middleware": "^3.4.1",
    "i18next-node-fs-backend": "^2.1.3",
    "jwt-decode": "^4.0.0",
    "laravel-passport-express": "git+https://github.com/Waavi/laravel-passport-express.git#master",
    "moment": "^2.29.4",
    "node-machine-id": "^1.1.12",
    "path-browserify": "^1.0.1",
    "promise-retry": "^2.0.1",
    "queue-promise": "^2.2.1",
    "socket.io": "^4.4.1",
    "socketio-jwt": "^4.6.2",
    "utf-8-validate": "^6.0.3",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-port": "^1.1.0",
    "winston": "^3.11.0"
  }
}
